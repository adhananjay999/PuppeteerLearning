{
  "compilerOptions": {
    "target": "es6" ,
    "module": "commonjs" ,
    "outDir": "./dist" ,
    "rootDir": "./src" ,
    "strict": true ,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true, 
    "noUnusedParameters": false, 
    "esModuleInterop": true ,
    "experimentalDecorators": true ,"emitDecoratorMetadata": true,
    "skipLibCheck": true ,"forceConsistentCasingInFileNames": true
  }
}
